<h1>Places</h1>

<% if @places.any? %>
  <ul>
    <% @places.each do |place| %>
      <li><%= link_to place.name, place_path(place) %></li>
    <% end %>
  </ul>
<% else %>
  <p>No places found.</p>
<% end %>

<% if current_user %>
  <p>
    <%= link_to "New Place", new_place_path %>
  </p>
<% end %>
